#!/bin/sh
me=$(realpath "$0")
program=$(basename "$me")
installation=$(dirname "$(dirname "$me")")
executable="$installation/usr/bin/$program"
if [ -x "$executable" ] ; then
  exec "$executable" \
       --configuration none \
       --progress true \
       --server.endpoint tcp://127.0.0.1:8529 \
       --server.authentication true \
       "$@"
fi
echo "Could not find executable!"
exit 1
